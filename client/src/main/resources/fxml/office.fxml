<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<TabPane xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.tuwien.sepm.assignment.group02.client.gui.OfficeFXML">
   <tabs>
      <Tab text="Bestellungen">
         <content>
            <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
                <children>
                    <TextField fx:id="selectedOrder" layoutX="168.0" layoutY="14.0" prefHeight="25.0" prefWidth="85.0" promptText="select order" />
                  <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#createOrder" text="Bestellung erstellen" />
                    <Button fx:id="deleteOrderButton" layoutX="168.0" layoutY="49.0" mnemonicParsing="false" onAction="#deleteOrder" text="Delete Order" />
                </children>
            </AnchorPane>
         </content>
      </Tab>
   </tabs>
</TabPane>
